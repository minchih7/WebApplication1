
@{
    ViewData["Title"] = "Categoryapi";
}

<h1>Categoryapi</h1>
<buton class="btn btn-danger">Click</buton>



@section Scripts{
    <script>
        const btn = document.querySelector(".btn")
        btn.addEventListener('click',async()=>{
            const response = await fetch('https://localhost:7162/api/Categories/')
            const datas = await response.json();
            console.log(datas);
        })
    </script>
}

